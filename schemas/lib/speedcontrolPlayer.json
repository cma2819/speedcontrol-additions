{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "name": {
            "type": "string"
        },
        "id": {
            "type": "string"
        },
        "teamID": {
            "type": "string"
        },
        "country": {
            "type": "string"
        },
        "social": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "twitch": {
                    "type": "string"
                }
            }
        },
        "externalID": {
            "type": "string"
        }
    },
    "required": [
      "name",
      "id",
      "teamID",
	  "social",
	  "externalID"
    ]
}
