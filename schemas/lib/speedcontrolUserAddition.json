{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "id": {
            "type": "string"
        },
        "social": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "nico": {
                    "type": "string"
                },
                "youtube": {
                    "type": "string"
                },
                "twitter": {
                    "type": "string"
                }
            }
        }
    },
    "required": ["id", "social"]
}