{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "id": {
            "type": "string"
        },
        "assignedRunIdArray": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "name": {
            "type": "string"
        },
        "social": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "twitch": {
                    "type": "string"
                },
                "nico": {
                    "type": "string"
                },
                "youtube": {
                    "type": "string"
                },
                "twitter": {
                    "type": "string"
                }
            }
        }
    },
    "required": [
        "id",
        "assignedRunIdArray",
        "name",
        "social"
    ]
}